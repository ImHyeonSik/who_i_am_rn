{
  "name": "who_i_am_rn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "reset": "watchman watch-del-all && react-native start --reset-cache",
    "android": "react-native run-android",
    "aos:clean": "cd android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "ios:clean": "cd ios && rm -rf Pods && pod cache clean â€”all && arch -x86_64 pod install && cd ..",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --check ./src",
    "prepare": "husky install",
    "gradlew": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "package:init": "rm -rf node_modules && npm install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --ext .tsx --ext .ts src/ --fix"
    ],
    "./src/**": [
      "prettier --write ."
    ]
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.5.4",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/stack": "^6.3.12",
    "lint-staged": "^11.1.4",
    "react": "17.0.2",
    "react-native": "0.68.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.19.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.67.0",
    "prettier": "^2.8.3",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-navigation)/)"
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "assetsTransformer.js"
  ]
}
